  .react-directory-add-file-icon { display: none; }
}

.react-directory-remove-file-icon { display: block; }

@media screen and (max-width: 1278px) {
  .react-directory-remove-file-icon { display: none; }
}

.manifest-commit-form { margin-top: 16px; }

